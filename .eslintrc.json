{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {"jsx": true}
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": "webpack.config.js",
    "rules": {
        "no-unused-vars":"warn",
        "no-console":"error",
        "quotes":["warn", "single"],
        "jsx-quotes":["warn", "prefer-double"],
        "prefer-const":"warn",
        "indent": ["warn", 2],
        "comma-dangle":["warn", "only-multiline"],
        "semi":["warn", "always"]
    }
}
